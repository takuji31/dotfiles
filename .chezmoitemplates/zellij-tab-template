    default_tab_template {
        pane size=1 borderless=true {
            plugin location="tab-bar"
        }
        children
        pane size=1 borderless=true {
            plugin location="https://github.com/dj95/zjstatus/releases/latest/download/zjstatus.wasm" {
                format_left  "{mode} #[fg=blue,bold]{session}"
                format_center ""
                format_right "{command_git_branch}{datetime}"
                format_space ""

                mode_normal       "#[bg=green,fg=black,bold] NORMAL "
                mode_locked       "#[bg=red,fg=black,bold] LOCKED "
                mode_pane         "#[bg=blue,fg=black,bold] PANE "
                mode_tab          "#[bg=blue,fg=black,bold] TAB "
                mode_resize       "#[bg=yellow,fg=black,bold] RESIZE "
                mode_scroll       "#[bg=magenta,fg=black,bold] SCROLL "
                mode_session      "#[bg=magenta,fg=black,bold] SESSION "
                mode_move         "#[bg=yellow,fg=black,bold] MOVE "
                mode_search       "#[bg=magenta,fg=black,bold] SEARCH "
                mode_enter_search "#[bg=magenta,fg=black,bold] SEARCH "
                mode_rename_tab   "#[bg=yellow,fg=black,bold] RENAME TAB "
                mode_rename_pane  "#[bg=yellow,fg=black,bold] RENAME PANE "

                command_git_branch_command "git rev-parse --abbrev-ref HEAD"
                command_git_branch_format "#[fg=blue] {stdout} "
                command_git_branch_interval "10"
                command_git_branch_rendermode "static"

                datetime          "#[fg=white]{format}"
                datetime_format   "%Y-%m-%d %H:%M"
                datetime_timezone "Asia/Tokyo"
            }
        }
        pane size=1 borderless=true {
            plugin location="status-bar"
        }
    }
